{
  "version": 2,
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js"
    }
  ],
  "env": {
    "PORT": "4000",
    "FRONTEND_ORIGIN": "http://localhost:3000",
    "SUPABASE_URL": "@supabase_url",
    "SUPABASE_SERVICE_ROLE_KEY": "@supabase_service_role_key",
    "SUPABASE_ANON_KEY": "@supabase_anon_key",
    "SUPABASE_JWT_SECRET": "@supabase_jwt_secret",
    "LINE_LOGIN_CHANNEL_ID": "@line_login_channel_id",
    "LINE_LOGIN_CHANNEL_SECRET": "@line_login_channel_secret",
    "LIFF_ID": "@liff_id",
    "LINE_MESSAGING_CHANNEL_ID": "@line_messaging_channel_id",
    "LINE_MESSAGING_CHANNEL_SECRET": "@line_messaging_channel_secret",
    "LINE_MESSAGING_ACCESS_TOKEN": "@line_messaging_access_token",
    "RECEIPT_BASE_URL": "@receipt_base_url",
    "BOOKING_TIMEBLOCK_MINUTES": "60"
  }
}
